{
    "name": "cotapreco/kernel",
    "description": "Experimental stuff on top of `Symfony/HttpKernel`",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrey K. Vital",
            "email": "andreykvital@gmail.com",
            "homepage": "https://github.com/CentaurWarchief"
        }
